CREATE USER 'admin'@'%' IDENTIFIED  BY 'passwors';
GRANT ALL PRIVILEGES ON *.* TO 'admin'@'%' WITH GRANT OPTION;
FLUSH PRIVILEGES;
CREATE DATABASE employees;
USE employees;
CREATE TABLE employee(
emp_id VARCHAR(20),
first_name VARCHAR(20),
last_name VARCHAR(20),
pri_skill VARCHAR(20),
location VARCHAR(20));
